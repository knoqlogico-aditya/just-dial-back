

<div class="container my-5">
    <h1 class="text-center mb-4">List of Businesses</h1>
    <div class="row">
        <% if (businesses && businesses.length> 0) { %>
            <% businesses.forEach(business => { %>
                <div class="col-md-6 col-lg-4 mb-4">
                    <div class="card h-100 shadow-sm">
                        <img src="data:image/jpeg;base64," class="card-img-top" alt="Business Image">
                        <div class="card-body">
                            <h5 class="card-title">Business Name: <%= business.business_name %></h5>
                            <h5 class="card-text">
                                <strong>Address:</strong> <%= business.address %><br>
                                <strong>Phone:</strong> <%= business.phone %>
                            </h5>
                            <!-- View Details Button -->
                            <a href="business-details/<%business.id%>" class="btn btn-primary btn-sm">View Details</a>
                            <!-- Rating Button aligned at the bottom -->
                            <button class="btn btn-outline-secondary btn-sm rating-button">3.1⭐</button>
                        </div>
                    </div>
                </div>
            <% }) %>

                    <% } else { %>
                        <div class="col-12">
                            <p class="text-center">No businesses found.</p>
                        </div>
                        <% } %>
    </div>
</div>